#include<stdio.h>

int main()
{
float millimeter, centimeter, meter, kilometer;

print("enter the length in kilometers::")
scanf("%f",&kilometer);

meter = kilometer * 1000.0;
centimeter = kilometer * 100000.0;
millimeter = kilometer * 1000000.0;

printf("\n %.2f kilometer = %.2f meters", kilometer,meter);
printf("\n %.2f kilometer = %.2f centimeter", kilometer,centimeter);
printf("\n %.2f kilometer = %.2f millimeter", kilometer,millimeter);
return 0:
